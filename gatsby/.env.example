# This file should be copied to `.env.production` or `.env.development` for customized settings

# Google Analytics code
ANALYTICS_UA=""

# App path prefix, always starting but not ending with '/' (ex: "/my-blog")
# Leave empty ("") when deploying to the root
PATH_PREFIX=""

# Base URL of the generated site, without the path prefix and without ending '/'
BASE_URL="https://localhost:9000"

# Use of Workbox (https://developers.google.com/web/tools/workbox) for off-line content
OFFLINE_PWA="false"

# When `false`, commits from old builds will not be preserved on GitHub Pages
# (see https://github.com/tschaub/gh-pages#optionshistory)
GH_PAGES_HISTORY="false"

# Other settings for GH-Pages
GH_PAGES_BRANCH="gh-pages"
GH_PAGES_REMOTE="origin"

# Facebook app id (used in share button)
FACEBOOK_ID="xxx"

# Base URL for JClic projects library, without ending '/'
REPO_BASE="https://clic.xtec.cat/projects"

# Full URL to projects list
REPO_LIST="https://clic.xtec.cat/projects/projects.json"

# Link to the JNLP JClic Installer
JNLP_INSTALLER="https://clic.xtec.cat/jnlp/jclic/install.jnlp?argument=%%FILE%%"

# API entry point of the JClic repo search service
JCLIC_SEARCH_SERVICE="https://clic.xtec.cat/db/repo-search"

# Base URL for JClic user projects, without ending '/'
USERS_BASE="https://clic.xtec.cat/users"

# Google OAuth2 API id
GOOGLE_OAUTH2_ID=""

# API base for user's library
USERLIB_API="https://clic.xtec.cat/db"

# URL used as proxy base with `gatsby develop`
PROXY_BASE=""
