  <activity class="@text.FillInBlanks" name="Guerra Civil Espanyola" code="" autoJump="true">
   <description />
   <messages>
    <cell type="initial">
     <style />
     <p>Per cada pregunta tens tres respostes, tria la bona.</p>
    </cell>
