  <activity class="@panels.InformationScreen" name="anim.ass">
   <messages>
    <cell type="initial">
     <style />
     <p>Multimèdia i animacions</p>
    </cell>
    <cell type="final">
     <style />
    </cell>
   </messages>
   <settings margin="8" report="false">
    <container bgColor="0xC0C0C0">
