  <activity class="@associations.ComplexAssociation" name="ass02.ass" code="">
   <description>
    <p>Associazione  tra testi, con assegnazione di relazioni e soluzione.</p>
   </description>
   <messages>
    <cell type="initial">
     <style>
      <font family="Arial" size="13" bold="true" />
     </style>
     <p>Questa è un'associazione tra due testi, ma con un numero  di elementi differente per ciascuna griglia.</p>
    </cell>
    <cell type="final">
     <style>
      <font family="Arial" size="13" bold="true" />
     </style>
     <p>Molto bene!</p>
    </cell>
   </messages>
   <settings margin="8" report="true" reportActions="false">
    <helpWindow showSolution="true" />
    <container bgColor="0xC0C0C0">
     <counters time="true" actions="true" score="true" />
    </container>
    <window bgColor="0xC0C0C0" border="true" />
   </settings>
   <cells rows="3" cols="4" cellWidth="100.0" cellHeight="90.0" border="true" id="primary">
    <style>
     <font family="Arial" size="18" />
     <color foreground="0x0000FF" background="0xFFFFFF" />
    </style>
    <shaper class="@Rectangular" cols="4" rows="3" />
    <cell id="0">
     <p>gioco</p>
    </cell>
    <cell id="2">
     <p>vedrete</p>
    </cell>
    <cell id="2">
     <p>canterà</p>
    </cell>
    <cell id="1">
     <p>camminavano</p>
    </cell>
    <cell id="0">
     <p>guardate</p>
    </cell>
    <cell id="1">
     <p>disse</p>
